    DllStructSetData($tspt, "DataTransferLength", 0x8)
    DllStructSetData($tspt, "TimeOutValue", 0x15180)
    DllStructSetData($tspt, "DataBufferOffset", $isize)
    DllStructSetData($tspt, "SenseInfoOffset", 0x0)
