        If $iubound > +0xffffffff Then
            If $idirection Then
                For $i = $iubound To 0x1 Step +0xffffffff
                    $aarray[$i] = $aarray[$i + 0xffffffff]
                Next
                $aarray[0x0] = $vvalue
