    EndIf
    If DllStructGetSize($tresult) < ($asize[0x0] + $asize[0x1]) Then Return SetError(0x2, 0x0, 0x0)
    _WinAPI_MoveMemory($tresult, $tstruct1, $asize[0x0])
    _WinAPI_MoveMemory(DllStructGetPtr($tresult) + $asize[0x0], $tstruct2, $asize[0x1])
