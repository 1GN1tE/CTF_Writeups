    Local $hobj, $aret, $itype, $ierror = 0x0
    Select
        Case IsString($vid)
            $aret = DllCall("ole32.dll", "long", "CLSIDFromString", "wstr", $vid, "ptr", DllStructGetPtr($tfidd, 0x3))
            If @error Or $aret[0x0] Then
