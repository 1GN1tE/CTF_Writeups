    If $iwidth = +0xffffffff Then
        $iwidth = DllStructGetData($tobj, "bmWidth")
    EndIf
    If $iheight = +0xffffffff Then
        $iheight = DllStructGetData($tobj, "bmHeight")
    EndIf
    $aret = DllCall("user32.dll", "handle", "GetDC", "hwnd", 0x0)
    Local $hdc = $aret[0x0]
