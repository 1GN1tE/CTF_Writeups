        Else
            For $i = 0x0 To $iubound - $iubounds
                $aarray[$i] = $aarray[$i + $iubounds]
            Next
            For $i = 0x0 To $iubounds + 0xffffffff
                $aarray[$i + $iubound - $iubounds + 0x1] = $vvalue[$i]
